generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  username String @unique
  password String
  mobile   String @unique @db.Varchar(10)
  cardId   String @unique @db.Varchar(13)
  status   Boolean
}
